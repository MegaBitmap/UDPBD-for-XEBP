<Window x:Class="UDPBD_for_XEB__GUI.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UDPBD_for_XEB__GUI"
        mc:Ignorable="d"
        Title="About UDPBD for XEB+" WindowStartupLocation="CenterOwner" Height="400" Width="650" ResizeMode="NoResize" FontSize="16" FontWeight="Bold">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF2ABB5B" Offset="1"/>
            <GradientStop Color="#FFACF57C" Offset="0"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <TextBlock Text="Big Thanks to these Devs!" FontSize="24" TextAlignment="Center" Margin="32" Width="300" Height="32" VerticalAlignment="Top" Panel.ZIndex="5"/>
        <TextBox Name="TextBoxAbout" TextAlignment="Center" Margin="30"/>
    </Grid>
</Window>
